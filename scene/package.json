{
  "name": "bevy-ui-scene",
  "version": "1.0.0",
  "description": "UI Scene shows a functional UI for the Bevy Explorer",
  "scripts": {
    "start": "sdk-commands start",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "export-static": "sdk-commands export-static --destination static/ --realmName BevyUiScene --baseUrl https://dclexplorer.github.io/bevy-ui-scene/ --commsAdapter ws-room:ws-room-service.decentraland.org/rooms/bevy-ui-scene",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "format": "prettier --config .prettierrc -c \"./src/**/*.{ts,tsx}\"",
    "format:fix": "prettier -w --config .prettierrc \"./src/**/*.{ts,tsx}\"",
    "upgrade-sdk": "npm install @dcl/sdk@protocol-squad @dcl/js-runtime@protocol-squad @dcl/react-ecs@protocol-squad @dcl/sdk-commands@protocol-squad --save-exact"
  },
  "devDependencies": {
    "@dcl/js-runtime": "7.8.2-14265125083.commit-3001941",
    "@dcl/react-ecs": "7.8.2-14265125083.commit-3001941",
    "@dcl/sdk": "7.8.2-14265125083.commit-3001941",
    "@dcl/sdk-commands": "7.8.2-14265125083.commit-3001941",
    "@dcl/ecs": "7.8.2-14265125083.commit-3001941",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@dcl-sdk/utils": "^1.2.8",
    "dcl-npc-toolkit": "^1.2.2",
    "dcl-ui-toolkit": "^0.10.0",
    "fp-future": "^1.0.1"
  }
}