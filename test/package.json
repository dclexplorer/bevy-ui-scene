  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.4",
    "chokidar-cli": "^3.0.0",
    "mocha": "^10.8.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.5.0"
  }
}
